@{
    ViewData["Title"] = "Home Page";
}

<h1>爬取書籍資料</h1>

<div>
    <h2>用戶列表</h2>
    <table>
        <thead>
            <tr>
                <th>User ID</th>
                <th>User Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in ViewBag.Users)
            {
                <tr>
                    <td>@user.UserId</td>
                    <td>@user.UserName</td>
                </tr>
            }
        </tbody>
    </table>
</div>

<h2>書籍資料</h2>
<pre>@ViewBag.JsonData</pre>